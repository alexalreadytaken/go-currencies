{
    "schemes": [
        "http"
    ],
    "swagger": "2.0",
    "info": {
        "title": "Currencies service api",
        "contact": {},
        "version": "1.0"
    },
    "basePath": "/api",
    "paths": {
        "/btcusdt": {
            "get": {
                "produces": [
                    "application/json"
                ],
                "summary": "get last BTC USDT course",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/rest.BtcUsdtCourseSlice"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/rest.UnexpectedResult"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/rest.UnexpectedResult"
                        }
                    }
                }
            },
            "post": {
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "get BTC USDT course history with pagination",
                "parameters": [
                    {
                        "description": "pagination",
                        "name": "pagination",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/rest.BtcUsdtPaginationRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/rest.BtcUsdtHistoryPage"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/rest.UnexpectedResult"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/rest.UnexpectedResult"
                        }
                    }
                }
            }
        },
        "/currencies": {
            "get": {
                "produces": [
                    "application/json"
                ],
                "summary": "get last fiat to rub course",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/rest.FiatToAnyCourseSlice"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/rest.UnexpectedResult"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/rest.UnexpectedResult"
                        }
                    }
                }
            },
            "post": {
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "get fiat to rub course history with pagination",
                "parameters": [
                    {
                        "description": "pagination",
                        "name": "pagination",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/rest.FiatPaginationRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/rest.FiatToAnyHistoryPage"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/rest.UnexpectedResult"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/rest.UnexpectedResult"
                        }
                    }
                }
            }
        },
        "/latest": {
            "get": {
                "produces": [
                    "application/json"
                ],
                "summary": "get last fiat to BTC course",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/rest.FiatToAnyCourseSlice"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/rest.UnexpectedResult"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/rest.UnexpectedResult"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "rest.BtcUsdtCourseSlice": {
            "type": "object",
            "properties": {
                "timestamp": {
                    "type": "integer"
                },
                "value": {
                    "type": "number"
                }
            }
        },
        "rest.BtcUsdtHistoryPage": {
            "type": "object",
            "properties": {
                "history": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/rest.BtcUsdtCourseSlice"
                    }
                },
                "total": {
                    "type": "integer"
                }
            }
        },
        "rest.BtcUsdtPaginationRequest": {
            "type": "object",
            "properties": {
                "from_timestamp": {
                    "type": "integer"
                },
                "limit": {
                    "type": "integer"
                },
                "offset": {
                    "type": "integer"
                }
            }
        },
        "rest.FiatPaginationRequest": {
            "type": "object",
            "properties": {
                "from_date": {
                    "type": "string"
                },
                "limit": {
                    "type": "integer"
                },
                "offset": {
                    "type": "integer"
                }
            }
        },
        "rest.FiatToAnyCourseSlice": {
            "description": "has additional fields consist of currency code and amount, but cant image here",
            "type": "object",
            "properties": {
                "base_currency": {
                    "type": "string"
                },
                "date": {
                    "type": "string"
                }
            }
        },
        "rest.FiatToAnyHistoryPage": {
            "type": "object",
            "properties": {
                "history": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/rest.FiatToAnyCourseSlice"
                    }
                },
                "total": {
                    "type": "integer"
                }
            }
        },
        "rest.UnexpectedResult": {
            "type": "object",
            "properties": {
                "message": {
                    "type": "string"
                }
            }
        }
    }
}